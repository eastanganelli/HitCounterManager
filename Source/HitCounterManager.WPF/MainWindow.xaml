<wpf:FormsApplicationPage x:Class="HitCounterManager.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpf="clr-namespace:Xamarin.Forms.Platform.WPF;assembly=Xamarin.Forms.Platform.WPF"
        xmlns:common="clr-namespace:HitCounterManager.Common;assembly=HitCounterManager"
        xmlns:views="clr-namespace:HitCounterManager.Views;assembly=HitCounterManager"
        mc:Ignorable="d">
    <wpf:FormsApplicationPage.Title>
        <MultiBinding StringFormat="{}{0} {1}">
            <Binding Source="{x:Static common:Statics.ApplicationTitle}" />
            <Binding Source="WPF" />
        </MultiBinding>
    </wpf:FormsApplicationPage.Title>
</wpf:FormsApplicationPage>
